
<%- include("partials/header"); %>

<h1>Home</h1>

<p><%=StartingContent%></p>


<% postValues.forEach(function(value){ %>
    <h1><%= value.title %></h1>
    <% if(value.body.length >= 100){ %>
        <p><%= value.body.substr(0,100) + "..."%><a href="/posts/<%=value.title%>">Read more</a></p>
   <% } %>
    
<% }) %>


<%- include("partials/footer"); %>